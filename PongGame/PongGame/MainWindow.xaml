        <Rectangle x:Name="p1p3" Fill="White" HorizontalAlignment="Left" Height="30" Margin="314,463,0,0" Stroke="Black" VerticalAlignment="Top" Width="30"/>
        <Rectangle x:Name="p1p4" Fill="White" HorizontalAlignment="Left" Height="30" Margin="279,463,0,0" Stroke="Black" VerticalAlignment="Top" Width="30"/>
        <Rectangle x:Name="p1p5" Fill="White" HorizontalAlignment="Left" Height="30" Margin="244,463,0,0" Stroke="Black" VerticalAlignment="Top" Width="30"/>
        <Rectangle x:Name="p2p1" Fill="White" HorizontalAlignment="Left" Height="30" Margin="476,463,0,0" Stroke="Black" VerticalAlignment="Top" Width="30"/>
        <Rectangle x:Name="p2p2" Fill="White" HorizontalAlignment="Left" Height="30" Margin="511,463,0,0" Stroke="Black" VerticalAlignment="Top" Width="30"/>
        <Rectangle x:Name="p2p3" Fill="White" HorizontalAlignment="Left" Height="30" Margin="546,463,0,0" Stroke="Black" VerticalAlignment="Top" Width="30"/>
        <Rectangle x:Name="p2p4" Fill="White" HorizontalAlignment="Left" Height="30" Margin="581,463,0,0" Stroke="Black" VerticalAlignment="Top" Width="30"/>
        <Rectangle x:Name="p2p5" Fill="White" HorizontalAlignment="Left" Height="30" Margin="616,463,0,0" Stroke="Black" VerticalAlignment="Top" Width="30"/>
        <Label x:Name="TF_Timer" Content="00:00" HorizontalAlignment="Left" Margin="419,463,0,0" VerticalAlignment="Top" Height="30" Width="52" FontSize="16"/>
    </Grid>
</Window>
